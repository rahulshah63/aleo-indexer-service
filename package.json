{
  "name": "aleo-indexer",
  "version": "1.0.0",
  "description": "Indexer for Aleo.",
  "type": "module",
  "bin": {
    "aleo-indexer": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts dev",
    "build": "tsc",
    "text": "vitest",
    "test:typecheck": "vitest --typecheck.only",
    "start": "node dist/index.js start",
    "db:generate": "drizzle-kit generate",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "aleo",
    "indexer",
    "drizzle",
    "hono",
    "vite"
  ],
  "dependencies": {
    "@babel/code-frame": "^7.27.1",
    "@commander-js/extra-typings": "^12.1.0",
    "@hono/node-server": "^1.11.2",
    "async-retry": "^1.3.3",
    "axios": "^1.7.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.4.0",
    "hono": "^4.4.7",
    "http-terminator": "^3.2.0",
    "p-limit": "^5.0.0",
    "pg": "^8.12.0",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "prom-client": "^15.1.3",
    "stacktrace-parser": "^0.1.11",
    "vite": "^5.3.1",
    "vite-node": "^1.6.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/babel__code-frame": "^7.0.6",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.22.7",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.14"
  }
}
